<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">

    <item app:tsquare_state_holiday="true" android:state_selected="false">
        <color android:color="#FCCAC9"/>
    </item>

    <item app:tsquare_state_holiday="true" android:state_selected="true">
        <color android:color="#FCCAC9"/>
    </item>

    <item android:state_selected="true">
        <color android:color="@color/calendar_selected_day_bg"/>
    </item>
    <item android:state_pressed="true">
        <color android:color="@color/calendar_selected_day_bg"/>
    </item>
    <item app:tsquare_state_current_month="false">
        <color android:color="@color/calendar_inactive_month_bg"/>
    </item>
    <item app:tsquare_state_highlighted="true">
        <color android:color="#CFE5FD"/>
    </item>
    <item app:tsquare_state_today="true">
        <color android:color="@color/calendar_text_active"/>
    </item>
    <item>
        <color android:color="#666666"/>
    </item>
</selector>
